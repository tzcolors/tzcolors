{
  "name": "tzcolors",
  "version": "0.0.0",
  "scripts": {
    "actions:deploy": "ng deploy --name $GITHUB_ACTOR --email $GITHUB_ACTOR@users.noreply.github.com --no-silent",
    "actions:prettier": "prettier \"src/**/*.ts\" \"e2e/**/*.ts\" \"angular.json\" \"README.md\" --list-different",
    "postinstall": "node scripts/patch_crypto.js && npm run prepare-prod-build",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write './**/*'",
    "prepare-prod-build": "npm run disable-pure-getters && npm run configure-mangle",
    "disable-pure-getters": "replace 'pure_getters: buildOptions.buildOptimizer' 'pure_getters: false' ./node_modules/@angular-devkit/build-angular/src/webpack/configs/common.js ",
    "configure-mangle": "replace 'safari10: true' 'safari10: true, keep_fnames: true' ./node_modules/@angular-devkit/build-angular/src/webpack/configs/common.js "
  },
  "private": true,
  "dependencies": {
    "@airgap/beacon-sdk": "2.3.1",
    "@angular/animations": "~11.1.2",
    "@angular/cdk": "^11.2.1",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/forms": "~11.1.2",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@swimlane/ngx-charts": "^17.0.0",
    "@taquito/beacon-wallet": "^8.0.1-beta.1",
    "bignumber.js": "^9.0.1",
    "bootstrap": "^4.5.3",
    "bs58check": "^2.1.2",
    "fast-deep-equal": "^3.1.3",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-moment": "^5.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "~11.1.4",
    "@angular/compiler-cli": "~11.1.2",
    "@ngrx/schematics": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "@types/bs58check": "^2.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "replace": "^1.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.3"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "printWidth": 80
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
